if two sum is sorted
use binary search approach:

class Solution {
    public int[] twoSum(int[] num, int target) {
        int sum=0,left=0,right=num.length-1;
        while(left<right){
            sum=num[left]+num[right];
            if(sum==target){
                return new int[]{left+1,right+1};
            }else if(sum>target){
                right--;
            }else{
                left++;
            }
        }
        return new int[]{-1,-1};
    }
}
